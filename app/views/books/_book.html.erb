<div id="<%= dom_id book %>">
  <div>
    <strong>Title:</strong>
    <%= book.title %>
  </div>
  <div>
    <strong>Author:</strong>
    <%= book.author %>
  </div>
  <div>
    <strong>Publisher:</strong>
    <%= book.publisher %>
  </div>
  <div>
    <strong>Year:</strong>
    <%= book.year %>
  </div>
  <div>
    <strong>Edition:</strong>
    <%= book.edition %>
  </div>
  <div>
    <strong>Genre:</strong>
    <%= book.genre %>
  </div>
  <div>
    <strong>Description:</strong>
    <%= book.description %>
  </div>
  <div>
    <strong>Url:</strong>
    <%= book.url %>
  </div>
  <div>
    <strong>User:</strong>
    <%= book.user_id %>
  </div>
</div>
